---
import TextSection from './TextSection.astro';
import HeroSection from './HeroSection.astro';
const sections = Astro.props.sections;
---

{sections.map((section, index) => {
  switch (section._type) {
    case 'textSection':
      return <TextSection key={`text-${index}`} {...section} />;
    case 'heroSection':
      return <HeroSection key={`hero-${index}`} {...section} />;
  }
})}
